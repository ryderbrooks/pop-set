    "chai": "^4.2.0",
    "coveralls": "^3.0.3",
    "eslint": "^5.15.3",
    "eslint-plugin-tslint": "^3.1.0",
    "eslint-plugin-typescript": "^0.14.0",
    "mocha": "^7.2.0",
    "nyc": "^13.3.0",
    "shx": "^0.3.2",
    "tslint": "^5.14.0",
    "typedoc": "^0.14.2",
